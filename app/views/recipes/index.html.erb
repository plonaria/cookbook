  <%=  "Searching Fom"  %>
  <%= form_with url: "/search", method: :get do |form| %>
    <%= form.label :cuisine, "Cuisine:" %>
    <%= form.text_field :cuisine %>
    <br><br>
    <%= form.label :ingredient, "Ingredient" %>
    <%= form.text_field :ingredient %>
    <button type="submit" name="button">Submit</button>
  <% end %>
